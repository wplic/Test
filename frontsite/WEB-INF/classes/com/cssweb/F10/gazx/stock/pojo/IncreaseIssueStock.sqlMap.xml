<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN"
		"http://www.ibatis.com/dtd/sql-map-2.dtd">
<!-- iBatis SQL Map 文件 -->	
<sqlMap namespace="com.cssweb.F10.gazx.stock.pojo.IncreaseIssueStock">
    <typeAlias alias="increaseIssueStock" type="com.cssweb.F10.gazx.stock.pojo.IncreaseIssueStock"/>
    <!--result map-->
    <resultMap id="increaseIssueStockMap" class="increaseIssueStock">
        <result column="ZFJC" property="increaseIssueAbbreviation" jdbcType="String" nullValue=""/>
        <result column="Zfjg" property="increaseIssuePrice" jdbcType="double"/>
		<result column="ZFND" property="increaseIssueYear" jdbcType="java.util.Date" nullValue="1900/01/01 00:00:00"/>
        <result column="T_GPZB_ID" property="stockId" jdbcType="Long"/>
		<result column="zfssr" property="increaseIssueDate" jdbcType="java.util.Date" nullValue="1900/01/01 00:00:00"/>
        <result column="MZJE" property="increaseIncome" jdbcType="double"/>
    </resultMap> 
    
	<select id="pageQuery" resultMap="increaseIssueStockMap">
		SELECT * FROM ( 
			SELECT row_.*, ROWNUM rownum_ FROM ( 
			     SELECT
						ZFJC,
						Zfjg,
						ZFND,
						T_GPZB_ID,
						zfssr,
						MZJE
				FROM ZF
				where SCBZ = 0 and GKBZ = 1
				<isNotNull prepend="and" property="stockId">
					T_GPZB_ID = #stockId#
				</isNotNull>
		) row_ WHERE ROWNUM &lt;= #endNum# ) r WHERE
		rownum_ &gt;#beginNum#
	</select>
</sqlMap>	