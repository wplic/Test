<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN"
		"http://www.ibatis.com/dtd/sql-map-2.dtd">

<!-- iBatis SQL Map 文件 -->
<sqlMap namespace="com.cssweb.rzrq.pojo.PledgeQuoteLog">

	<typeAlias alias="pledgeQuoteLog" type="com.cssweb.rzrq.pojo.PledgeQuoteLog" />

	<resultMap id="pledgeQuoteLogMap" class="pledgeQuoteLog">
		<result property="id" column="ID" jdbcType="Long"/>
		<result property="mainId" column="MAIN_ID" jdbcType="Long"/>
		<result property="code" column="CODE" jdbcType="String"/>
		<result property="name" column="NAME" jdbcType="String" />
	    <result property="rzRate" column="RZ_RATE" jdbcType="Double" />
	    <result property="rqRate" column="RQ_RATE" jdbcType="Double" />
	    <result property="userId" column="USER_ID" jdbcType="Long"/>
	   <result property="userName" column="USER_NAME" jdbcType="String"/>
	    <result property="updateDate" column="UPDATE_DATE" jdbcType="Date"/>
	</resultMap>
	
	<select id="getPledgeQuoteLogByMainId" parameterClass="Long" resultMap="pledgeQuoteLogMap">
		select * from T_RZRQ_PLEDGEQUOTE_LOG where MAIN_ID = #mainId#
	</select>
	

	<insert id="insertPledgeQuoteLog" parameterClass="pledgeQuoteLog">
		INSERT into T_RZRQ_PLEDGEQUOTE_LOG ( ID, MAIN_ID,CODE,NAME,RZ_RATE,RQ_RATE,USER_ID,USER_NAME,UPDATE_DATE) VALUES 
		(#id#,#mainId#, #code#,#name#,#rzRate#,#rqRate#,#userId#,#userName#,#updateDate#)
	</insert>
	
	
	
	<select id="getCountBySql" resultClass="java.lang.Integer" parameterClass="String">
		$sql$
	</select>
	
	  <select id="getPledgeQuoteLogByPage" parameterClass="java.util.Map" resultMap = "pledgeQuoteLogMap">
    SELECT * FROM ( SELECT row_.*, ROWNUM rownum_ FROM ( $sql$ )
		row_ WHERE ROWNUM &lt;= #end# ) WHERE rownum_ &gt;#begin#
  </select>
  
  
</sqlMap>
