<!-- iBatis SQL Map 文件 -->
<sqlMap namespace="com.cssweb.hr.pojo.HrUserWorkExperience">
				 
	<typeAlias alias="HrUserWorkExperience" type="com.cssweb.hr.pojo.HrUserWorkExperience"/>
								
	<!--result map-->
	<resultMap id="resultMap" class="HrUserWorkExperience">
		<result property="id" column="ID" jdbcType="Long"/>
 		<result property="userId" column="USER_ID" jdbcType="Long"/>
 		<result property="startDateYear" column="STARTTIME_YEAR" jdbcType="String"/>
 		<result property="startDateMonth" column="STARTTIME_MONTH" jdbcType="String" />
 		<result property="endDateYear" column="ENDTIME_YEAR" jdbcType="String" />
 		<result property="endDateMonth" column="ENDTIME_MONTH" jdbcType="String" />
 		<result property="companyName" column="COMPANYNAME" jdbcType="String" />
 		<result property="companyScope" column="COMPANYSCOPE" jdbcType="String" />
 		<result property="workStyle" column="WORKSTYLE" jdbcType="String" />
 		<result property="workDept" column="WORKDEPT" jdbcType="String" />
 		<result property="workPosition" column="WORKPOSITION" jdbcType="String" />
 		<result property="laborage" column="LABORAGE" jdbcType="String" />
 		<result property="workResponse" column="WORKRESPONSE" jdbcType="String" />
 		<result property="mainResult" column="MAINRESULT" jdbcType="String" />
 		<result property="reportto" column="REPORTTO" jdbcType="String" />
 		<result property="underling" column="UNDERLING" jdbcType="String" />
 		<result property="prover" column="PROVER" jdbcType="String" />
 		<result property="prorelation" column="PRORELATION" jdbcType="String" />
 		<result property="proverphone" column="PROVERPHONE" jdbcType="String" />
 		<result property="lzyy" column="LZYY" jdbcType="String" />
 		
 	</resultMap>
 	
  	<insert id="insert" parameterClass="HrUserWorkExperience">
		<![CDATA[
		 insert into T_HR_USER_WORKEXPERIENCE(ID,USER_ID,STARTTIME_YEAR,STARTTIME_MONTH,ENDTIME_YEAR,ENDTIME_MONTH,COMPANYNAME,
		 		COMPANYSCOPE,WORKSTYLE,WORKDEPT,WORKPOSITION,LABORAGE,WORKRESPONSE,MAINRESULT,REPORTTO,UNDERLING,PROVER,PRORELATION,PROVERPHONE,LZYY)
		 	 values(#id#,#userId#,#startDateYear#,#startDateMonth#,#endDateYear#,#endDateMonth#,
		 		#companyName#,#companyScope#,#workStyle#,#workDept#,#workPosition#,
		 		#laborage#,#workResponse#,#mainResult#,#reportto#,#underling#,#prover#,
		 		#prorelation#,#proverphone#,#lzyy#
		 		)
		]]>
	</insert>
	
	<update id="update" parameterClass="HrUserWorkExperience">
		update T_HR_USER_WORKEXPERIENCE
		 set 
		 	USER_ID=#userId#,STARTTIME_YEAR=#startDateYear#,STARTTIME_MONTH=#startDateMonth#,
		 	ENDTIME_YEAR=#endDateYear#,ENDTIME_MONTH=#endDateMonth#,COMPANYNAME=#companyName#,
		 	COMPANYSCOPE=#companyScope#,WORKSTYLE=#workStyle#,WORKDEPT=#workDept#,WORKPOSITION=#workPosition#,
		 	LABORAGE=#laborage#,WORKRESPONSE=#workResponse#,MAINRESULT=#mainResult#,REPORTTO=#reportto#,
		 	UNDERLING=#underling#,PROVER=#prover#,PRORELATION=#prorelation#,PROVERPHONE=#proverphone#,LZYY=#lzyy#
		where ID = #id#
	</update>
	
	<delete id="delete" parameterClass="Long">
		delete from T_HR_USER_WORKEXPERIENCE where USER_ID = #userId#
	</delete>
	
	<delete id="deleteById" parameterClass="Long">
		delete from T_HR_USER_WORKEXPERIENCE where ID = #id#
	</delete>
	
	<select id="getAllWorkList" parameterClass="Long" resultMap="resultMap">
   		select 
   			*
		    from T_HR_USER_WORKEXPERIENCE 
		    where 
		     user_id=#userId#
		    order by STARTTIME_YEAR desc,STARTTIME_MONTH desc
   </select>
   
	<select id="getBeanById" parameterClass="Long" resultMap="resultMap">
   		select 
   			*
		    from T_HR_USER_WORKEXPERIENCE 
		    where 
		     id=#id#
   </select>
   
   
</sqlMap>
