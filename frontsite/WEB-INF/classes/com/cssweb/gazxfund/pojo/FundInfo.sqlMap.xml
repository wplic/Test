<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN"
		"http://www.ibatis.com/dtd/sql-map-2.dtd">
		
<!-- iBatis SQL Map 文件 -->
<sqlMap namespace="com.cssweb.gazxfund.pojo.FundInfo">
				 
	<typeAlias alias="fundInfo" type="com.cssweb.gazxfund.pojo.FundInfo"/>
										
	<!--result map-->
	<resultMap id="fundInfoMap" class="fundInfo">
 		<result property="jjdm" column="JJDM" jdbcType="String"/>
 		<result property="jjjc" column="JJJC" jdbcType="String"/>
 		<result property="gsmc" column="GSMC" jdbcType="String" />
 		<result property="jjzgm" column="JJZGM" jdbcType="Long" />
 		<result property="cxqj" column="CXQJ" jdbcType="String" />
 		<result property="dqrq" column="DQRQ" jdbcType="java.util.Date" />
 		
 	</resultMap>
 	
  <select id="getAllCountBySql" parameterClass="java.lang.String" resultClass = "Integer">
    $sql$
  </select>
  
  <select id="getFundInfoPage" parameterClass="java.util.Map" resultMap = "fundInfoMap">
    SELECT * FROM ( SELECT row_.*, ROWNUM rownum_ FROM ( $sql$ )
		row_ WHERE ROWNUM &lt;= #end# ) WHERE rownum_ &gt;#begin#
  </select>
  
</sqlMap>
