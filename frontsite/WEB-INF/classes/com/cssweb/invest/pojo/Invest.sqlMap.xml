	<insert id="insertInvestBasic" parameterClass="Invest">
		INSERT INTO INVEST_BASIC ( ID , USERID , TITLE ,BEGIN_TIME , END_TIME ,INVEST_NUM , COL_NAME , STATUS , DELFLAG ,DEPARTMENT , PIC , MEMO ,SYSTIME , TYPE , EXT1 , EXT2) 
		   VALUES (#id#, #userid#, #title#, #beginTime#, #endTime#, #investNum#, #colName#, #status#, #delflag#, #department#, #pic#, #memo#, sysdate, #type#, #ext1#, #ext2#)
	</insert>
	
	<insert id="insertInvestAnswer" parameterClass="Invest">
		INSERT INTO INVEST_ANSWER ( ID , QUESTION_ID ,ANSWER , DELFLAG , INVEST_ID) 
		   VALUES (#id#, #questionId#, #answer#, #delflag#, #investId#)
	</insert>
	
	<insert id="insertInvestQuestion" parameterClass="Invest">
		INSERT INTO INVEST_QUESTION ( ID , INVEST_ID ,QUESTION , QUESTION_TYPE ,ANSWER_NUM , DELFLAG) 
		   VALUES (#id#, #investId#, #question#, #questionType#, #answerNum#, #delflag#)
	</insert>
	
	<insert id="insertInvestData" parameterClass="Invest">
		INSERT INTO INVEST_DATA ( USERID , USERNAME , INVEST_ID ,QUESTION_ID , ANSWER_TYPE , CONTENT , SAVEDATE ,EXT1 , EXT2 , EXT3 , EXT4) 
		   VALUES (#userid#, #username#, #investId#, #questionId#, #answerType#, #content#, #savedate#, #ext1#, #ext2#, #ext3#, #ext4#)
	</insert>
	
	<insert id="insertInvestUser" parameterClass="Invest">
		INSERT INTO INVEST_USER ( USERID , USERNAME , INVEST_ID ,IP , ADDRESS , POSTCARD ,CARD , TELPHONE ,SAVEDATE ,EXT1 , EXT2 , EXT3 , EXT4) 
		   VALUES (#userid#, #username#, #investId#, #ip#, #address#, #postcard#,#card#, #telphone#, #savedate#, #ext1#, #ext2#, #ext3#, #ext4#)
	</insert>
	
