<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts-config PUBLIC
		"-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
		"http://struts.apache.org/dtds/struts-config_1_2.dtd">
<struts-config>
	
	<action-mappings>		
		<action path="/myquote"  scope="request" parameter="method" validate="false">
		  <forward name="list" path="/index.jsp"/>	
		  <forward name="myStockList" path="/bussinesshall/movetradehall/myStockList.jsp"/>		
		  <forward name="addMyStock" path="/bussinesshall/movetradehall/myStock.jsp"/>
		  <forward name="delStock" path="/myquote.do?method=getMyquoteList"/>	
		  <forward name="StockZcglList" path="/bussinesshall/movetradehall/stockZcglList.jsp"/>						
		</action>
		
	</action-mappings>
</struts-config>