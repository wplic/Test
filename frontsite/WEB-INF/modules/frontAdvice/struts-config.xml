<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts-config PUBLIC
		"-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
		"http://struts.apache.org/dtds/struts-config_1_2.dtd">
<struts-config>
	<form-beans>
		<form-bean name="adviceform" type="org.apache.struts.validator.LazyValidatorForm" />
	</form-beans>
	<global-forwards>
	</global-forwards>
	
	<action-mappings>
		 <action path="/advice" parameter="method" validate="false" name="adviceform">
			<forward name="success" path="/investmentBank/feedback.jsp"></forward>
		 </action>
	</action-mappings>
</struts-config>